local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()



local colors_theme = {
    SchemeColor = Color3.fromRGB(152, 252, 197),
    Background = Color3.fromRGB(23, 26, 25),
    Header = Color3.fromRGB(14, 15, 15),
    TextColor = Color3.fromRGB(33, 147, 105),
    ElementColor = Color3.fromRGB(20, 20, 20)
}

local Window = Library.CreateLib("Bangkok Dinner - Aisuki Hub  By : Staliliy (Hydrofrom)", colors_theme)

--x Discord Server Menu x--
local Tab2 = Window:NewTab("Discord")
local Section2 = Tab2:NewSection("**SOON you can join this discord team")
Section2:NewButton("[SOON!] Join Discord", "Join Discord Soon", function()
    print("can't use for now!")
end)



--x Main Server Laging Menu x--
local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("**Server Will Lag [100K EGGS], [1M EGGS]")
Section:NewToggle("100K EGGS", "ues for make give player 100K EGGS!", function(state)
    if state then
        local Players = game:GetService("Players")
 
local LocalPlayer = Players.LocalPlayer
 
while true do task.wait(1)
    local Passed, Statement = pcall(function()
        for i,v in next, Players:GetPlayers() do
            if v.Name ~= LocalPlayer.Name and v.Character and v.Character:FindFirstChild('RemoteEvent') then
                v.Character:FindFirstChild('RemoteEvent'):FireServer({
                    ["Other"] = {
                        ["Egg"] = 100000,
                    }
                })
            end
        end
    end)
 
    if not Passed then
        warn(Statement)
    end
end
    else
        print("Toggle Off")
    end
end)


Section:NewToggle("1M EGGS", "ues for make give player 1M EGGS!", function(state)
    if state then
        local Players = game:GetService("Players")
 
local LocalPlayer = Players.LocalPlayer
 
while true do task.wait(1)
    local Passed, Statement = pcall(function()
        for i,v in next, Players:GetPlayers() do
            if v.Name ~= LocalPlayer.Name and v.Character and v.Character:FindFirstChild('RemoteEvent') then
                v.Character:FindFirstChild('RemoteEvent'):FireServer({
                    ["Other"] = {
                        ["Egg"] = 1000000,
                    }
                })
            end
        end
    end)
 
    if not Passed then
        warn(Statement)
    end
end
    else
        print("Toggle Off")
    end
end)


local Tab3 = Window:NewTab("Settings")
local Section3 = Tab3:NewSection("Settings")
Section3:NewKeybind("Keybind", "KeybindInfo", Enum.KeyCode.F, function()
	Library:ToggleUI()
end)
Section3:NewSlider("SpeedWalk", "Set your speed walk", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
